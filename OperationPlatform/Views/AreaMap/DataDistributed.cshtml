                    }
                }
                //option.tooltip.formatter = '滚轮切换省份或点击返回全国<br/>{b},{a},{c}';
            } else {
                mt = 'china';
                //option.tooltip.formatter = '滚轮切换或点击进入该省<br/>{b},{a}';
            }
            MpaDataSelect(mt);
        });

        var data_extend = [
            //{ name: '南京市', value: 500 }
        ]

        //var label = '';

        var option = {
            title: {
                text: '全国34个省市自治区',
                subtext: 'china （点击切换）'
            },
            tooltip: {
                trigger: 'item',
                formatter: function(a) {
                    //alert(label);
                    res = a[1] + '</br>' + label + ':' + a[2];
                    for (var i = 0; i < data_extend.length; i++) {
                        if (a[1] == data_extend[i].name) {
                            res += '</br>扩展属性：' + data_extend[i].value;
                        }
                    }
                    return res;
                }
            },
            legend: {
                show: false,
                orient: 'vertical',
                x: 'right',
                data: ['分布']
            },
            dataRange: {
                min: 0,
                max: 3000,
                
                //color: ['green', '#EEEEEE'],
                //text: ['高', '低'], // 文本，默认为数值文本
                calculable: true
            },
            series: [
                {
                    name: '分布',
                    type: 'map',
                    mapType: 'china',
                    selectedMode: 'single',
                    itemStyle: {
                        normal: { label: { show: true } },
                        emphasis: { label: { show: true } }
