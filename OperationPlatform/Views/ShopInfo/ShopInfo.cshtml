                    <td class="sitd sititle">开店时长：</td>
                    <td class="sitd sicontent" id="sys_duration" onclick=" ColumnTdClick(this, 'Duration') ">&nbsp;</td>
                </tr>
                <tr>
                    <td class="sitd sititle">用过软件：</td>
                    <td class="sitd sicontent" colspan="3" id="sys_software" onclick=" ColumnTdClick(this, 'OtherSoftware') ">&nbsp;</td>
                    <td class="sitd sititle" colspan="6">&nbsp;</td>
                </tr>
                <tr>
                    <td class="sitd sititle">备注：</td>
                    <td class="sitd sicontent" colspan="9" id="sys_remark" onclick=" ColumnTdClick(this, 'Remark') ">&nbsp;</td>
                </tr>
            </table>
        </div>

        <hr>
        <div class="row">
            <div class="ButtonList m-l-15">
                <button class="btn btn-primary fs-12" onclick=" AppListShow() ">App开通管理</button>
                <button class="btn btn-complete fs-12 m-l-5" onclick=" GetSubranchList() ">分店信息</button>
                <button class="btn btn-complete fs-12 m-l-5" onclick=" GetAccountUserList() ">店员信息</button>
