                    <td class="sitd sicontent" id="sys_weixin" onclick=" ColumnTdClick(this, 'WeiXin') "></td>
                    <td class="sitd sititle">电话：</td>
                    <td class="sitd sicontent" id="sys_tel" onclick=" ColumnTdClick(this, 'Tel') "></td>
                    <td class="sitd sititle">店铺大小：</td>
                    <td class="sitd sicontent" id="sys_shopsize" onclick=" ColumnTdClick(this, 'ShopSize') ">&nbsp;</td>
                    <td class="sitd sititle">经营内容：</td>
                    <td class="sitd sicontent" id="sys_operate" onclick=" ColumnTdClick(this, 'Operate') ">&nbsp;</td>
                </tr>
                <tr>
                    <td class="sitd sititle">地址：</td>
                    <td class="sitd sicontent" id="sys_address" onclick=" ColumnTdClick(this, 'Address') ">&nbsp;</td>
                    <td class="sitd sititle">行业：</td>
                    <td class="sitd sicontent" id="sys_industry" onclick=" ColumnTdClick(this, 'Industry') ">&nbsp;</td>
