@model Model.T_AccountBasic
@{
    Layout = null;
    ViewBag.Title = "店铺信息";
}

<style type="text/css">
    .icon-multiple {
        background-color: #959595;
    }

    .icon-font {
        display: inline-block;
        color: #fff;
        font-style: normal;
        font-size: 12px;
        line-height: 16px;
        text-align: center;
        border-radius: 1px;
        margin-right: 3px;
        padding: 0px 1px 0px 2px;
        background-color: #f63;
        margin-top: 2px;
    }

    .icon-font-g {
        display: inline-block;
        color: #fff;
        font-style: normal;
        font-size: 12px;
        line-height: 16px;
        text-align: center;
        border-radius: 1px;
        margin-right: 3px;
        padding: 0px 1px 0px 2px;
        background-color: green;
        margin-top: 2px;
    }
</style>


<link rel="stylesheet" href="~/Scripts/plugins/bootstrap-tag/bootstrap-tagsinput.css">
<div class="panel no-border">
    <div class="panel-heading bg-white">
        <div class="panel-title">
            @if (Model.LoginTimeLast.ToShortDateString() == DateTime.Now.ToShortDateString())
            {
                <h5 class="no-margin inline text-key">@Html.DisplayFor(model => model.CompanyName)</h5>
            }
            else
            {
                <h5 class="no-margin inline">@Html.DisplayFor(model => model.CompanyName)</h5>
            }

            <span class="form-group">
                <span class="help"> [@Html.DisplayFor(model => model.ID)] </span>
            </span>
            @if (Model.aotjbName == "免费版")
            {
                <span class="m-l-20">@Html.DisplayFor(model => model.aotjbName)</span>
            }
            else
            {
                <span class="m-l-20 text-danger">@Html.DisplayFor(model => model.aotjbName)</span>
            }

            <span class="fs-12 hint-text m-l-20">活跃状态:</span>
            <span class="text-success fs-16 bold m-l-5 m-r-5">@Html.DisplayFor(model => model.activeName)</span>
            <span class="fs-12 hint-text m-l-20" id="behaveIndex"></span>
        </div>
        <div class="panel-controls pull-right">
            <button class="btn btn-complete btn-cons" onclick="AddVisitInfoButtonClick()">增加回访</button>
        </div>
        <div class="panel-controls pull-right ">
            <button class="btn btn-success btn-cons" onclick="SendSms()">发送短信</button>
        </div>

        <div class="panel-controls pull-right">
            <h5 id="userReturn" class="m-b-15 m-r-20">
                <span class="fs-12 hint-text">距上一次回访间隔</span><span class="period m-l-5 bold text-success"></span>天<span class="source m-l-5"></span>
            </h5>
        </div>

        @*<div class="clearfix"></div>*@
    </div>
    <div class="panel-body no-padding">
        <div class="row defaultFill padding-20 b-b b-t b-grey">
            <div class="col-xs-12 col-sm-6 col-md-3">
                <h5 class="m-t-0">
                    登录次数：<span class="text-success bold" style="cursor:help" id="logDetail">@Html.DisplayFor(model => model.LoginTimeWeb)</span>
                    <span id="loginTimeDiv" class="small-text"></span>
                </h5>

                <h6 class="m-b-0">最后登录：(@Html.DisplayFor(model => model.LoginTimeLast))<span id="lastClient"></span></h6>
            </div>
            @*<div class="col-xs-12 col-sm-6 col-md-4">
                    <h6 class="hint-text m-t-0">店铺版本</h6>
                    <h5 class="m-b-0">@Html.DisplayFor(model => model.aotjbName)</h5>
                </div>*@
            <div id="regTimeDiv" class="col-xs-12 col-sm-6 col-md-3">
                <h6 class="hint-text m-t-0">注册时间</h6>
                <h5 class="m-b-0"></h5>
            </div>
            <div id="endTimeDiv" class="col-xs-12 col-sm-6 col-md-3">
                <h6 class="hint-text m-t-0">到期时间</h6>
                <h5 class="m-b-0"></h5>
            </div>
            <div id="orderNum" class="col-xs-12 col-sm-6 col-md-3">
                <h6 class="hint-text m-t-0">订单</h6>
                <h5 class="m-b-0"></h5>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3 padding-20 shop-profiles b-r b-grey">
                <div class="row">
                    <p class="pull-left">店主名称</p>
                    <p class="pull-right bold">@Html.DisplayFor(model => model.UserRealName)</p>
                </div>
                <div class="row">
                    <p class="pull-left">登录账号</p>
                    @if (!string.IsNullOrEmpty(Model.PhoneNumber))
                    {
                        <p class="pull-right bold">@Html.DisplayFor(model => model.PhoneNumber)</p>
                    }
                    else
                    {
                        <p class="pull-right bold">@Html.DisplayFor(model => model.UserEmail)</p>
                    }

                </div>
                @if (Model.ServiceManager == "邮件自助注册" && !string.IsNullOrEmpty(Model.PhoneNumber))
                {
                    <div class="row">
                        <p class="pull-left">邮箱地址</p>
                        <p class="pull-right bold">@Html.DisplayFor(model => model.UserEmail)</p>
                    </div>
                }
                <!-- <div class="row">
                    <p class="pull-left">电子邮件</p>
                    <p class="pull-right bold">@Html.DisplayFor(model => model.UserEmail)</p>
                </div> -->
                <div class="row">
                    <p class="pull-left">店铺简称</p>
                    <p class="pull-right bold">@Html.DisplayFor(model => model.shotName)</p>
                </div>

                <div class="row">
                    <p class="pull-left">客户经理</p>
                    <p class="pull-right bold">@Html.DisplayFor(model => model.ServiceManager)</p>
                </div>
                <div class="row">
                    <p class="pull-left">代理商</p>
                    @if (!string.IsNullOrEmpty(Model.AgentName))
                    {
                        <p class="pull-right bold">@Html.DisplayFor(model => model.AgentName)</p>
                    }
                    else
                    {
                        <p class="pull-right bold">无信息</p>
                    }

                </div>
                @*<div class="row no-border">
                        <div class="col-xs-6 col-sm-6 col-md-6 no-padding p-r-20">
                            <div class="row">
                                <p class="pull-left">注册时间</p>
                                <p class="pull-right bold">@Html.DisplayFor(model => model.RegTime)</p>
                            </div>
                        </div>
                        <div class="col-xs-6 col-sm-6 col-md-6 no-padding">
                            <div class="row">
                                <p class="pull-left">到期时间</p>
                                @if (Model.endtime == DateTime.MinValue)
                                {
                                    <p class="pull-right bold">无限时</p>
                                }
                                else
                                {
                                    <p class="pull-right bold">@Html.DisplayFor(model => model.endtime)</p>
                                }

                            </div>
                        </div>
                    </div>*@
                @*<div class="row no-border">

                        <div class="col-xs-6 col-sm-6 col-md-6 no-padding p-r-20">
                            <div class="row">
                                <p class="pull-left">客户经理</p>
                                <p class="pull-right bold">@Html.DisplayFor(model => model.ServiceManager)</p>
                            </div>
                        </div>
                        <div class="col-xs-6 col-sm-6 col-md-6 no-padding">
                            <div class="row">
                                <p class="pull-left">代理商</p>
                                <p class="pull-right bold">@Html.DisplayFor(model => model.AgentName)</p>
                            </div>
                        </div>
                    </div>*@
            </div>
            <div id="AccountInfo" class="col-xs-3 col-sm-3 col-md-3 padding-20 shop-profiles">
                @*<div class="row">
                        <p class="pull-left">活跃等级</p>
                        <p class="pull-right bold">@Html.DisplayFor(model => model.activeName)</p>
                    </div>*@
                @*<div class="row">
                        <p class="pull-left">总积分</p>
                        <p class="pull-right text-danger bold">@Html.DisplayFor(model => model.integral)</p>
                    </div>
                    <div class="row">
                        <p class="pull-left">短信</p>
                        <p class="pull-right bold">剩余：<span class="text-key m-r-5">@Html.DisplayFor(model => model.smsSurplusNum)</span>条，已发送<span id="smsSendAllNum" class="m-l-5">@Html.DisplayFor(model => model.smsNum)</span> 条</p>
                    </div>*@
                <div class="clearfix"></div>
            </div>
            <div class="col-xs-6 col-sm-6 col-md-6 padding-20 b-l b-grey">
                <div class="row" id="AccountAddress">
                    <p>
                        商铺地址:
                        @if (Model.CompanyAddress != "" && Model.CompanyAddress != null)
                        {
                            <span class="bold m-l-5">@Html.DisplayFor(model => model.CompanyAddress)</span>
                        }
                        else
                        {
                            <span class="bold m-l-5">未填写地址</span>
                        }
                        （<span class="bold m-l-5">推断地址:</span><span class="aboutAddrees bold m-l-5"></span>）
                    </p>
                    @*<p class="bold">@Html.DisplayFor(model => model.CompanyAddress)</p>*@
                    @*<p>推断地址:<span class="aboutAddrees bold m-l-5"></span></p>*@
                </div>
                <div class="row m-t-0" id="sysHuiFangHelp">
                    <p>用户后台信息:<span class="m-l-5 bold txtContent"></span></p>
                </div>
                <div class="row">
                    <hr>
                    <p class="pull-left inline m-r-5">
                        用户标签：
